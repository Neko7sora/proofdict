{
  "name": "@proofdict/tester-cli",
  "version": "1.0.0",
  "description": "A CLI for @proofdict/tester.",
  "keywords": [
    "cli",
    "proofdict"
  ],
  "homepage": "https://github.com/proofdict/proofdict/tree/master/packages/@proofdict/tester-cli/",
  "bugs": {
    "url": "https://github.com/proofdict/proofdict/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "bin": {
    "proofdict-tester": "./bin/cmd.js"
  },
  "main": "lib/tester-cli.js",
  "types": "lib/tester-cli.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proofdict/proofdict.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "cross-env": "^5.1.4",
    "mocha": "^5.0.4",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "dependencies": {
    "@proofdict/tester": "^1.0.0",
    "@proofdict/types": "^1.0.0",
    "@types/globby": "^6.1.0",
    "@types/js-yaml": "^3.10.1",
    "@types/meow": "^4.0.1",
    "globby": "^8.0.1",
    "js-yaml": "^3.11.0",
    "meow": "^4.0.0"
  }
}