{
  "name": "@proofdict/migrate",
  "version": "3.0.3",
  "description": "Migration tool for proofdict.",
  "keywords": [
    "migrate",
    "migration",
    "proofdict"
  ],
  "homepage": "https://github.com/proofdict/proofdict/tree/master/packages/@proofdict/migrate/",
  "bugs": {
    "url": "https://github.com/proofdict/proofdict/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/cli.js",
  "types": "lib/cli.d.ts",
  "bin": {
    "proofdict-migrate": "./bin/cmd.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proofdict/proofdict.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "cross-env UPDATE_SNAPSHOT=1 mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@proofdict/domain": "^3.0.0",
    "glob": "^5.0.15",
    "js-yaml": "^3.12.0",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.11.2",
    "@types/meow": "^5.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
