{
  "name": "proofdict",
  "version": "1.2.1",
  "private": true,
  "description": "Proofdict is a collection of dictionary.",
  "keywords": [
    "dict"
  ],
  "homepage": "https://github.com/proofdict/proofdict",
  "bugs": {
    "url": "https://github.com/proofdict/proofdict/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "dict/",
    "public/dict.json",
    "index.js"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proofdict/proofdict.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "clearn": "lerna run clearn",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prepublish": "yarn run clearn && yarn run build",
    "publish": "lerna publish --conventional-commits",
    "test": "lerna run test",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'"
  },
  "dependencies": {},
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "globby": "^6.1.0",
    "husky": "^0.14.3",
    "lerna": "^2.9.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4
  },
  "workspaces": [
    "packages/*",
    "packages/@proofdict/*"
  ]
}
